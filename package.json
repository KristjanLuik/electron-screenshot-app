{
  "name": "electron-screenshot-app",
  "version": "1.0.0",
  "description": "electron-screenshot-app",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "start-ln":"electron main.js --enable-transparent-visuals --disable-gpu",
