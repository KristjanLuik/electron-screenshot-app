  },
  "homepage": "https://github.com/KristjanLuik/electron-screenshot-app#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "electron-prebuilt": "^1.2.7"
  },
  "dependencies": {
    "electron-json-storage": "^2.0.0",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^6.0.4",
    "highlight.js": "^9.3.0"
  }
